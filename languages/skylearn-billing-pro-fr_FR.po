msgid "Value Added Tax (VAT)"
msgstr "Taxe sur la Valeur Ajoutée (TVA)"

msgid "Goods and Services Tax (GST)"
msgstr "Taxe sur les Produits et Services (TPS)"

msgid "Harmonized Sales Tax (HST)"
msgstr "Taxe de Vente Harmonisée (TVH)"

msgid "Provincial Sales Tax (PST)"
msgstr "Taxe de Vente Provinciale (TVP)"

msgid "Quebec Sales Tax (QST)"
msgstr "Taxe de Vente du Québec (TVQ)"

msgid "Sales Tax"
msgstr "Taxe de Vente"

msgid "Service Tax"
msgstr "Taxe de Service"

msgid "Other"
msgstr "Autre"

# Notifications
msgid "Payment Success"
msgstr "Paiement Réussi"

msgid "Sent when a payment is successfully processed"
msgstr "Envoyé quand un paiement est traité avec succès"

msgid "Payment Failed"
msgstr "Paiement Échoué"

msgid "Sent when a payment fails"
msgstr "Envoyé quand un paiement échoue"

msgid "Subscription Created"
msgstr "Abonnement Créé"

msgid "Sent when a new subscription is created"
msgstr "Envoyé quand un nouvel abonnement est créé"

msgid "Subscription Cancelled"
msgstr "Abonnement Annulé"

msgid "Sent when a subscription is cancelled"
msgstr "Envoyé quand un abonnement est annulé"

msgid "Subscription Renewed"
msgstr "Abonnement Renouvelé"

msgid "Sent when a subscription is renewed"
msgstr "Envoyé quand un abonnement est renouvelé"

msgid "Course Enrollment"
msgstr "Inscription au Cours"

